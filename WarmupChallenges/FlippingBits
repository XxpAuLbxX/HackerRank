import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

// Created class
public class Solution {
    
	private static int size = 0;
	
    public static void main(String[] args) {
        /* Reads input from STDIN. Print output to STDOUT. */
        try{
		BufferedReader br = 
                      new BufferedReader(new InputStreamReader(System.in));
 
		String input;
        Boolean flag = false; // Checks first input
        int[] bits = new int[0];
        int i = 0;
 
		while((input=br.readLine())!=null) {
            if(!flag) {
            	
                size = Integer.parseInt(input);
                bits = new int[size];
                flag = true;
                continue;
                
            }
			bits[i] = Integer.parseInt(input);
            i++;
		}
		
		bitFlipper(bits);
 
        }catch(IOException io){
        	io.printStackTrace();
        }
        
    }
    
    public static void bitFlipper(int[] bits) {
        int i = 0; 
        int num, result, mult;
        while(i < size) {
            
            num = bits[i];
            result = 0;
            mult = 1;
            
            while(num > 0) {
                int mod = num % 2;
                num /= 2;
                if(mod == 0) {
                    result = result + 1 * mult;
                } else if (mod == 1) {
                    result = result + 0 * mult;
                }
                mult *= 10;
            }
            System.out.println(result);
            i++;
            }
        }
        
    }

